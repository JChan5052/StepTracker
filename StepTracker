public class StepTracker{
private int active=0;
private int limit=0;
private int steps=0;
    public void StepTracker(int lim){
        limit=lim;
    }

public int activeDays()
{
return active;
}
public void addDailySteps(int steps){
    if (steps>limit){
        this.steps+=steps;
        active++;
    }
}
}